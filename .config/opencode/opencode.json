{
  "$schema": "https://opencode.ai/config.json",
  "model": "google/gemini-2.5-pro",
  "provider": {
    "ollama": {
      "npm": "@ai-sdk/openai-compatible",
      "options": {
        "baseURL": "http://localhost:11434/v1"
      },
      "models": {
        "qwen:agentic": {
          "name": "Qwen Agentic",
          "tool_call": false,
          "reasoning": false
        },
        "gpt-oss:latest": {
          "name": "Open AI",
          "tool_call": true,
          "reasoning": true
        }
      }
    }
  }
}
